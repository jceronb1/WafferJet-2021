<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_Agregar_Vehiculo"
    android:layout_margin="25dp"
    android:background="@drawable/fondo">

    <TextView
        android:id="@+id/Uvallet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="81dp"
        android:layout_marginLeft="97dp"
        android:layout_marginTop="-2dp"
        android:layout_marginEnd="81dp"
        android:layout_marginRight="82dp"
        android:background="#000C0808"
        android:text="U-Vallet"
        android:textColor="#3F51B5"
        android:textSize="55sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/agregarVehiculoText"
        android:layout_width="138dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Uvallet"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="81dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="-5dp"
        android:layout_marginEnd="80dp"
        android:layout_marginRight="100dp"
        android:text="Agregar Vehiculo"
        android:textColor="#050505"
        android:textSize="25sp"
        android:textStyle="bold" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="156dp"
        android:layout_height="98dp"
        android:layout_below="@+id/agregarVehiculoText"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="54dp"
        android:layout_marginLeft="54dp"
        android:layout_marginTop="40dp"
        android:clickable="true"
        app:backgroundTint="#86847F"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/fotoPerfilText"
        android:layout_width="215dp"
        android:layout_height="30dp"
        android:layout_below="@+id/agregarVehiculoText"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="65dp"
        android:layout_toEndOf="@+id/floatingActionButton2"
        android:layout_toRightOf="@+id/floatingActionButton2"
        android:text="Agregue fotos del vechiculo" />

    <TextView
        android:id="@+id/PlacaView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fotoPerfilText"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="116dp"
        android:layout_marginRight="116dp"
        android:text="Placa del Vehiculo"
        android:textColor="#000000"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/PlacaText"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_below="@+id/PlacaView"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="-2dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="" />


    <EditText
        android:id="@+id/nombreCompletoText"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_below="@+id/nombreCompletoView"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="-2dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="36dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="" />

    <TextView
        android:id="@+id/nombreCompletoView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/PlacaText"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="29dp"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="120dp"
        android:layout_marginRight="120dp"
        android:text="Nombre Completo"
        android:textColor="#000000"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/modeloVehiculoText"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_below="@+id/modeloView"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="-2dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="32dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="" />

    <TextView
        android:id="@+id/modeloView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/nombreCompletoText"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="116dp"
        android:layout_marginRight="116dp"
        android:text="Modelo del Vehiculo"
        android:textColor="#000000"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/CapacidadText"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_below="@+id/CapacidadView"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="-2dp"
        android:layout_marginTop="-2dp"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="32dp"
        android:background="#FFFFFF"
        android:ems="10"
        android:inputType="" />

    <TextView
        android:id="@+id/CapacidadView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/modeloVehiculoText"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="33dp"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="116dp"
        android:layout_marginRight="116dp"
        android:text="Capacidad del Vehiculo"
        android:textColor="#000000"
        android:textSize="16sp" />

    <Button
        android:id="@+id/guardarVehiculo"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_below="@+id/CapacidadText"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="117dp"
        android:layout_marginLeft="117dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="29dp"
        android:layout_marginRight="29dp"
        android:background="#EEBC0B"
        android:text="Guardar"
        app:backgroundTint="#EEBC0B" />

</RelativeLayout>